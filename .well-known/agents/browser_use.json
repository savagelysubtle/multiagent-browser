{
  "name": "Browser Use Agent",
  "description": "Browser automation agent for navigation, extraction, and screenshots.",
  "url": "https://webui.example.com/a2a/agents/browser_use",
  "version": "1.0.0",
  "provider": {
    "name": "web-ui",
    "contact": "simpleflowworks@gmail.com",
    "url": "https://github.com/savagelysubtle/web-ui"
  },
  "documentationUrl": "https://github.com/savagelysubtle/web-ui/blob/main/backend/src/web_ui/agent/A2A_AGENT_CARDS.md#browser-use-agent",
  "capabilities": {
    "streaming": false,
    "pushNotifications": false,
    "stateTransitionHistory": false
  },
  "defaultInputModes": [
    "application/json"
  ],
  "defaultOutputModes": [
    "application/json"
  ],
  "skills": [
    {
      "id": "browse",
      "name": "Browse",
      "description": "Navigate to a URL and interact with the page per natural language instructions.",
      "tags": ["web", "navigation"],
      "examples": [
        "browse:{\"url\":\"https://example.com\",\"instruction\":\"Collect the hero headline\"}"
      ],
      "inputModes": [
        "application/json"
      ],
      "outputModes": [
        "application/json"
      ]
    },
    {
      "id": "extract",
      "name": "Extract",
      "description": "Collect structured data from a webpage using CSS selectors.",
      "tags": ["web", "scraping"],
      "examples": [
        "extract:{\"url\":\"https://example.com\",\"selectors\":[\"article h2\"]}"
      ],
      "inputModes": [
        "application/json"
      ],
      "outputModes": [
        "application/json"
      ]
    },
    {
      "id": "screenshot",
      "name": "Screenshot",
      "description": "Capture a screenshot of a rendered page.",
      "tags": ["web", "screenshots"],
      "examples": [
        "screenshot:{\"url\":\"https://example.com\"}"
      ],
      "inputModes": [
        "application/json"
      ],
      "outputModes": [
        "application/json",
        "image/png"
      ]
    }
  ],
  "securitySchemes": {},
  "security": [],
  "metadata": {
    "browserAutomation": "Playwright",
    "supportsJavascriptExecution": true
  }
}
