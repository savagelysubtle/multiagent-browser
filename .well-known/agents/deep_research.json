{
  "name": "Deep Research Agent",
  "description": "Stateful research agent that synthesises findings across multiple sources.",
  "url": "https://webui.example.com/a2a/agents/deep_research",
  "version": "1.0.0",
  "provider": {
    "name": "web-ui",
    "contact": "simpleflowworks@gmail.com",
    "url": "https://github.com/savagelysubtle/web-ui"
  },
  "documentationUrl": "https://github.com/savagelysubtle/web-ui/blob/main/backend/src/web_ui/agent/A2A_AGENT_CARDS.md#deep-research-agent",
  "capabilities": {
    "streaming": false,
    "pushNotifications": false,
    "stateTransitionHistory": false
  },
  "defaultInputModes": [
    "application/json"
  ],
  "defaultOutputModes": [
    "application/json"
  ],
  "skills": [
    {
      "id": "research",
      "name": "Research",
      "description": "Conduct configurable depth research on a topic, returning findings and references.",
      "tags": ["research", "synthesis"],
      "examples": [
        "research:{\"topic\":\"AI alignment\",\"depth\":\"standard\"}"
      ],
      "inputModes": [
        "application/json"
      ],
      "outputModes": [
        "application/json"
      ]
    },
    {
      "id": "analyze_sources",
      "name": "Analyze Sources",
      "description": "Evaluate provided sources for credibility and relevance.",
      "tags": ["research", "analysis"],
      "examples": [
        "analyze_sources:{\"sources\":[\"https://example.com/blog\",\"https://arxiv.org/abs/1234\"]}"
      ],
      "inputModes": [
        "application/json"
      ],
      "outputModes": [
        "application/json"
      ]
    }
  ],
  "securitySchemes": {},
  "security": [],
  "metadata": {
    "langgraphIntegration": true,
    "supportsCitationManagement": true
  }
}
